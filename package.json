{
  "name": "worldcare-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "rootDir": "dist",
  "_moduleAliases": {
    "@app": "./",
    "@utils": "./utils",
    "@modules": "./modules",
    "@core": "./core"
  },
  "scripts": {
    "test": "TS_NODE_FILES=true mocha --exit -r dotenv/config -r ts-node/register -r tsconfig-paths/register  src/**/*.test.ts",
    "test-ci": "TS_NODE_FILES=true nyc --reporter=text mocha -r ts-node/register test/**/*.test.ts",
    "test-report": "TS_NODE_FILES=true nyc --reporter=html mocha --exit -r ts-node/register test/**/*.test.ts;",
    "build": "tsc; cp package.json dist;",
    "start:prod": "NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/index.js",
    "start": "ts-node-dev -r tsconfig-paths/register  --respawn --transpileOnly ./src/index.ts",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "eslint ./src --ext .js,.ts --fix",
    "apidoc": "npm run gen-apidoc; ts-node-dev -r tsconfig-paths/register  --respawn --transpileOnly ./src/apidoc.ts",
    "gen-apidoc": "apidoc -f '.*\\.controller.ts$' -i ./src -o ./apidoc",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "keywords": [
    "rpm",
    "backend"
  ],
  "author": "WorldTech Ltd.",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/bcrypt": "^3.0.1",
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.11",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/merge2": "^1.3.0",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.21",
    "@types/mongoose-aggregate-paginate-v2": "^1.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^4.21.0",
    "apidoc": "^0.25.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "mocha": "^7.1.2",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/elasticsearch": "^5.0.36",
    "@types/express-http-proxy": "^1.6.1",
    "@types/jaeger-client": "^3.15.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/node-fetch": "^2.5.7",
    "JSONStream": "^1.3.5",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "casbin": "^5.6.1",
    "casbin-mongoose-adapter": "^2.2.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "hash.js": "^1.1.7",
    "jaeger-client": "^3.18.0",
    "json-bigint": "^0.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "merge2": "^1.4.1",
    "migrate-mongo": "^8.2.2",
    "migrate-mongoose-typescript": "^3.3.4",
    "module-alias": "^2.2.2",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.9.15",
    "mongoose-aggregate-paginate-v2": "^1.0.42",
    "mongoose-intl": "^3.2.0",
    "mongoose-paginate-v2": "^1.3.9",
    "node-fetch": "^2.6.0",
    "serialize-javascript": "^5.0.1",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  }
}
